#include <iostream>

using namespace std;

class Animal {
public:
    void speak(){ cout << "Grrrr" << endl; }
};

class Cat: public Animal {
    
};

int main()
{
    Animal a;
    a.speak();
    
    Cat cat;
    cat.speak();
    
    return 0;
}
